version: '2'
services:
  tsn_haproxy:
    image: "{{ container_registry }}/contrail-external-haproxy:{{ contrail_version_tag }}"
    env_file: /etc/contrail/tsn_haproxy.env
    network_mode: "host"
    privileged: true
    pid: host
    volumes:
      - /dev/:/dev/
      - /var/lib/:/var/lib/
      - /etc/network:/etc/network
      - /var/run/:/var/run/
      - /run/:/run/
